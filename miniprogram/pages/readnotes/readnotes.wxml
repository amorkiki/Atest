<!--pages/readnotes/readnotes.wxml-->
<import src="../template/nav.wxml"></import>
<view class="page">
  <!-- 顶部导航模板 -->
  <template is="topNav" data="{{navTitle}}"></template>
  <!-- 轮播图 -->
  <view class="swiper-box">
    <swiper indicator-dots indicator-color="#CCDAFC" indicator-active-color="#7A7AF7" autoplay="{{true}}">
    <swiper-item>123</swiper-item>
    <swiper-item>234</swiper-item>
    <swiper-item>123</swiper-item>
  </swiper>
  </view>
  <!-- 笔记列表 -->
  <view class="booklist">
    <listCard titleName="{{titleName}}" subTitle="{{subTitle}}"></listCard>
  </view>
  <!-- 搜索按钮 -->
  <import src="../template/search.wxml"></import>
   <template is="search"></template>
  <!-- 添加按钮 -->
  <import src="../template/add.wxml"></import>
  <template is="add"></template>
</view>